    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#d4ebc8",
    "statusBar.background": "#b7dea2",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#9ad17c",
    "statusBarItem.remoteBackground": "#b7dea2",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#b7dea2",
